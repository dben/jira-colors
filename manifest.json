{
   "author": "David Benson",
   "host_permissions": [ "https://*.atlassian.net/jira/*" ],
   "content_scripts": [ {
      "js": [ "script.js" ],
      "matches": [ "https://*.atlassian.net/jira/*" ],
      "run_at": "document_end"
   } ],
   "description": "Highlights background color of issues",
   "icons": {
      "128": "icon_128.png"
   },
   "manifest_version": 3,
   "name": "JIRA Issue Highlighting",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3",
   "web_accessible_resources": [ 
      {
         "matches": [ "https://*.atlassian.net/*" ],
         "resources": [ "script.js" ]
      }
  ]
}
